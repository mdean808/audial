{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "lib": ["WebWorker"],
    "paths": {
      "$src/*": ["src/*"],
      "$lib":["src/lib"],
      "$lib/*":["src/lib/*"],
      "$components/*": ["src/components/*"],
      "$types/*": ["src/stores/*"],
      "$routes/*": ["src/actions/*"]
    }
  }
}
